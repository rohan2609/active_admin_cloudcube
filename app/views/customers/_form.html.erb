<%= simple_form_for(@customer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fullname %>
    <%= f.input :contact %>
    <%= f.input :email %>
    <%= f.input :access_token %>
    <%= f.input :customer %>
    <%= f.input :date %>
    <%= f.input :status %>
    <%= f.input :address %>
    <%= f.input :pincode %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
